{
  "name": "vue3-matomo",
  "version": "1.2.1",
  "description": "A Vue 3 and Nuxt 3 plugin for Matomo Analytics.",
  "license": "MIT",
  "readme": "./README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dankc/vue3-matomo.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "vue",
    "nuxt",
    "matomo",
    "analytics"
  ],
  "scripts": {
    "format": "prettier --write src/",
    "type-check:src": "tsc --project tsconfig.json --noEmit",
    "type-check:spec": "tsc --project tsconfig.spec.json --noEmit",
    "build": "npm run type-check:src && vite build",
    "pack": "npm run build; npm pack --pack-destination ./pack",
    "test": "vitest run"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "files": [
    "/dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@vitejs/plugin-vue": "^5.2.3",
    "happy-dom": "^17.4.7",
    "prettier": "^3.5.3",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "3.1.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "engines": {
    "node": ">= 20.0.0",
    "npm": ">= 11.0.0"
  },
  "bugs": {
    "url": "https://github.com/dankc/vue3-matomo/issues"
  },
  "homepage": "https://github.com/dankc/vue3-matomo#readme",
  "author": {
    "name": "Dan Kiser",
    "email": "dankc@pm.me"
  }
}
